<tr  class="border border-dark mt-1">
  <td><%= test.id %></td>
  <td><%= test.title %></td>
  <td><%= test.level %></td>
  <td><%= test.questions.count %></td>
  <td>
    <div class="btn-group" role="group" aria-label="Basic example">
      <%= link_to t('.show'), admin_test_path(test), class: 'btn btn-secondary' %>
      <%= link_to t('.edit'), edit_admin_test_path(test), class: 'btn btn-secondary' %>
      <%= link_to t('.delete'),
                      admin_test_path(test),
                      method: :delete,
                      data: { confirm: 'Are you sure?'},
                      class: 'btn btn-secondary'
          %>
    </div>
  </td>
</tr>
