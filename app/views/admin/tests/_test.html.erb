<tr>
  <td><%= test.id %></td>
  <td><%= test.title %></td>
  <td><%= test.level %></td>
  <td><%= test.questions.count %></td>

  <td><%= link_to I18n.t('admin.tests.test.show'), admin_test_path(test) %></td>
  <td><%= link_to I18n.t('admin.tests.test.edit'), edit_admin_test_path(test) %></td>
  <td>
    <%= link_to I18n.t('admin.tests.test.delete'),
                admin_test_path(test),
                method: :delete,
                data: { confirm: 'Are you sure?'}
    %>
  </td>

</tr>
